#include "logic.h"

SoundEffect::SoundEffect(const char* name_, Eff_CB cb, void* V_struct) : effect_function(cb), int_Vstr{} {
    strncpy(name, name_, sizeof(name));
    name[sizeof(name) - 1] = '\0';
}

float32_t gain_effect(float32_t sample, void* params2) {
	float gain = *(float*)params2;
    return sample * gain;
}
